<% title(t('.page_title')) %>
<%= render partial: 'back_link' %>

<%= form_for @issue, url: report_issue_step_path(params[:id]), :as => :issue do |f| %>
  <%= f.govuk_error_summary %>
  <%= f.govuk_text_field :org_name, width: 'two-thirds', label: {size: 'l', tag: 'h1'} %>
  <%= f.govuk_submit %>
<% end %>