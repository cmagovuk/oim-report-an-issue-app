
<% title(t('.page_title')) %>
<% content_for(:back_link) do%>
  <%= link_to t("link.back"), link_start_page, class: 'govuk-back-link ga-pageLink'%>
<% end %>
<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <h1 class="govuk-heading-l">
      <%= t('.heading') -%>
    </h1>
    <%= t('.privacy_notice_html', link_info_charter: "/pages/your_information") -%>
    <%= link_to report_issues_path, method: :post, class: 'govuk-button govuk-!-margin-top-2 govuk-!-margin-bottom-8', role:"button",draggable: "false", "data-module" => "govuk-button" do %>
      Continue
    <% end %>
  </div>
</div>
